{
    "name": "netgen/better-ibexa-admin-ui",
    "description": "Better Ibexa Admin UI implements an enhanced administration UI for Ibexa DXP",
    "type": "ezplatform-bundle",
    "homepage": "https://github.com/netgen/better-ibexa-admin-ui",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://netgen.io"
        }
    ],
    "require": {
        "ibexa/admin-ui": "^4.4"
    },
    "require-dev": {
        "netgen/ibexa-site-api": "^5.4",
        "ibexa/graphql": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\Bundle\\BetterIbexaAdminUIBundle\\": "bundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": false
    }
}
